{
    "name": "@secretary/aws-secrets-manager-adapter",
    "description": "AWS SecretsManager adapter for Secretary",
    "version": "2.0.0-alpha.0",
    "repository": {
        "type": "github",
        "url": "https://github.com/secretary/node"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "lint": "tslint --config tslint.json --project tsconfig.json -t stylish --fix",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && npx codecov",
        "prepublishOnly": "npm run build"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/**/*.spec.ts"
        ],
        "reporter": [
            "text"
        ],
        "all": true
    },
    "author": "Aaron Scherer <aequasi@gmail.com>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@secretary/core": "^2.0.0-alpha.0",
        "@types/chai-as-promised": "^7.1.0",
        "chai-as-promised": "^7.1.1",
        "typemoq": "^2.1.0"
    },
    "peerDependencies": {
        "aws-sdk": "^2.425.0"
    },
    "gitHead": "826e83a1ff5dbeee35cf36ed9b72f5fe4dfe83b9",
    "devDependencies": {
        "aws-sdk-mock": "^4.4.0",
        "typemoq": "^2.1.0"
    }
}
