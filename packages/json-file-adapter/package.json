{
    "name": "@secretary/json-file-adapter",
    "description": "JSON File adapter for Secretary",
    "version": "2.0.1",
    "repository": {
        "type": "github",
        "url": "https://github.com/secretarysecrets/node-json-file"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "lint": "tslint --config tslint.json --project tsconfig.json -t stylish --fix",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && npx codecov"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/**/*.spec.ts"
        ],
        "reporter": [
            "text"
        ],
        "all": true
    },
    "author": "Aaron Scherer <aequasi@gmail.com>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@secretary/core": "^2.0.1",
        "@types/chai-as-promised": "^7.1.0",
        "chai-as-promised": "^7.1.1",
        "mock-fs": "^4.9.0",
        "typemoq": "^2.1.0"
    },
    "gitHead": "826e83a1ff5dbeee35cf36ed9b72f5fe4dfe83b9",
    "devDependencies": {
        "@types/mock-fs": "^3.6.30",
        "mock-fs": "^4.9.0"
    }
}
